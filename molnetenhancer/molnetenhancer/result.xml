<?xml version="1.0" encoding="ISO-8859-1" ?>
<interface id="MOLNETENHANCER" version="1.2.3">
    <download>
            <source type="folder" name="output_network/"/>
    </download>


    <view id="clone_updated_networking" label="Clone Job to Current Workflow Version" group="Clone to Current Workflow Version">
        <blockRef id="main" type="clone_updated_networking"/>
    </view>
    <block id="clone_updated_networking" type="dummylinkout">
        <data>
            <parsers>
                <parser type="stream" contentType="text/xml"/>
            </parsers>
        </data>
        <parameter name="URLBASE" value='/ProteoSAFe/index.jsp?task=[task]&amp;params=%7B%22workflow%22%3A%22MOLNETENHANCER%22%2C%20%22version%22%3A%20%22current%22%7D'/>
    </block>

    <view id="download_data" label="Download Network">
        <blockRef id="main"   type="download_data"/>
    </view>
    <block id="download_data" type="dummydownload">
      <data>
        <source type="file" name="output_network/ClassyFireResults_Network.graphml"/>
         <parsers>
            <parser type="stream" contentType="text/xml"/>
        </parsers>
      </data>
    </block>

    <view id="cytoscape_preview" label="Direct Cytoscape Preview/Download" group="Advanced Views - Experimental Views">
            <blockRef id="main"   type="cytoscape_preview"/>
    </view>
    <block id="cytoscape_preview" type="dummylinkout">
      <data>
         <parsers>
            <parser type="stream" contentType="text/xml"/>
        </parsers>
      </data>
        <parameter name="URLBASE" value="http://dorresteinappshub.ucsd.edu:5051/process?task=[task]"/>
    </block>
    
</interface>
